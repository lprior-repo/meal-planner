{
  "name": "docs-scraper",
  "version": "1.0.0",
  "description": "Playwright-based documentation scraper for converting docs to markdown",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "scrape": "tsx src/index.ts",
    "scrape:profile": "tsx scripts/scrape-profile.ts",
    "scrape:recipes": "tsx scripts/scrape-recipes.ts",
    "build": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "playwright",
    "scraper",
    "documentation",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.41.0",
    "turndown": "^7.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.11.5",
    "@types/turndown": "^5.0.4",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
